// Write your own strlen() from memory.

#include <stdio.h>

int myStrlen(const char *str)
{
    int count = 0;
    const char *string = str;

    while (string != NULL)
    {
        string++;
        count++;
    
    }

    return count;
}

int main(void)
{
    const char *name = "Devil";
    printf("The string has %d characters", myStrlen(name));
    return 0;
}
